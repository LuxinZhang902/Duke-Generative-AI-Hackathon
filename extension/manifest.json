{
  "manifest_version": 2,
  "name": "Elucidate",
  "version": "1.0",
  "background": { "service_worker": "background.js" },
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "icons": {
    "16": "images/reading_16.png",
    "48": "images/reading_48.png",
    "128": "images/reading_128.png"
  },
  "web_accessible_resources": [
    "images/reading_16.png",
    "images/reading_48.png",
    "images/reading_128.png"
  ]
}
