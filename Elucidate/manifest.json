{
    "manifest_version": 2,
    "name": "Elucidate",
    "description": "Elucidate serves as a bridge to simplify and translate the online text, enhancing comprehension for non-native speakers and learners with varying levels of literacy.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
      }
    ],
    "icons": {
      "16": "images/reading_16.png",
      "48": "images/reading_48.png",
      "128": "images/reading_128.png"
    },
    "web_accessible_resources": [
      "images/reading_16.png",
      "images/reading_48.png",
      "images/reading_128.png"
    ]
  }